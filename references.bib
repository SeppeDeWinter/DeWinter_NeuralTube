@article{kimDNAguidedTranscriptionFactor2024,
  title = {{{DNA-guided}} Transcription Factor Cooperativity Shapes Face and Limb Mesenchyme},
  author = {Kim, Seungsoo and Morgunova, Ekaterina and Naqvi, Sahin and Goovaerts, Seppe and Bader, Maram and Koska, Mervenaz and Popov, Alexander and Luong, Christy and Pogson, Angela and Swigut, Tomek and Claes, Peter and Taipale, Jussi and Wysocka, Joanna},
