  file = {/Users/u0138640/Zotero/storage/LUPDG8QG/Yin et al. - 2024 - Iterative deep learning-design of human enhancers exploits condensed sequence grammar to achieve cel.pdf}
}

@article{zeitlingerSevenMythsHow2020,
  title = {Seven Myths of How Transcription Factors Read the Cis-Regulatory Code},
  author = {Zeitlinger, Julia},
