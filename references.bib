@article{zuinNonlinearControlTranscription2022,
  title = {Nonlinear Control of Transcription through Enhancer--Promoter Interactions},
  author = {Zuin, Jessica and Roth, Gregory and Zhan, Yinxiu and Cramard, Julie and Redolfi, Josef and Piskadlo, Ewa and Mach, Pia and Kryzhanovska, Mariya and Tihanyi, Gergely and Kohler, Hubertus and Eder, Mathias and Leemans, Christ and {van Steensel}, Bas and Meister, Peter and Smallwood, Sebastien and Giorgetti, Luca},
  year = {2022},
