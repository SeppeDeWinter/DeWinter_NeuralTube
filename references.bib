@misc{lalDecodingSequenceDeterminants2025,
  title = {Decoding Sequence Determinants of Gene Expression in Diverse Cellular and Disease States},
  author = {Lal, Avantika and Karollus, Alexander and Gunsalus, Laura and Garfield, David and Nair, Surag and Tseng, Alex M. and Gordon, M. Grace and Blischak, John and van de Geijn, Bryce and Bhangale, Tushar and Collier, Jenna L. and Diamant, Nathaniel and Biancalani, Tommaso and Bravo, Hector Corrada and Scalia, Gabriele and Eraslan, Gokcen},
