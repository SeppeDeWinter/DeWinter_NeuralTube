@article{bravogonzalez-blasSinglecellSpatialMultiomics2024,
  title = {Single-Cell Spatial Multi-Omics and Deep Learning Dissect Enhancer-Driven Gene Regulatory Networks in Liver Zonation},
  author = {{Bravo Gonz{\'a}lez-Blas}, Carmen and Matetovici, Irina and Hillen, Hanne and Taskiran, Ibrahim Ihsan and Vandepoel, Roel and Christiaens, Valerie and {Sansores-Garc{\'i}a}, Leticia and Verboven, Elisabeth and Hulselmans, Gert and Poovathingal, Suresh and Demeulemeester, Jonas and Psatha, Nikoleta and Mauduit, David and Halder, Georg and Aerts, Stein},
