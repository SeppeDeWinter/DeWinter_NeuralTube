@article{loubiereDevelopmentalHousekeepingTranscriptional2024,
  title = {Developmental and Housekeeping Transcriptional Programs Display Distinct Modes of Enhancer-Enhancer Cooperativity in {{Drosophila}}},
  author = {Loubiere, Vincent and {de Almeida}, Bernardo P. and Pagani, Michaela and Stark, Alexander},
  year = {2024},
