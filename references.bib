@article{taskiranCelltypedirectedDesignSynthetic2024,
  title = {Cell-Type-Directed Design of Synthetic Enhancers},
  author = {Taskiran, Ibrahim I. and Spanier, Katina I. and Dickm{\"a}nken, Hannah and Kempynck, Niklas and Pan{\v c}{\'i}kov{\'a}, Alexandra and Ek{\c s}i, Eren Can and Hulselmans, Gert and Ismail, Joy N. and Theunis, Koen and Vandepoel, Roel and Christiaens, Valerie and Mauduit, David and Aerts, Stein},
  year = {2024},
