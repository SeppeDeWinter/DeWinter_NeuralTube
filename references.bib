  file = {/Users/u0138640/Zotero/storage/FCS3P8ZS/Martinez-Ara et al. - 2024 - Large-scale analysis of the integration of enhancer-enhancer signals by promoters.pdf}
}

@article{martinez-araSystematicAnalysisIntrinsic2022,
  title = {Systematic Analysis of Intrinsic Enhancer-Promoter Compatibility in the Mouse Genome},
  author = {{Martinez-Ara}, Miguel and Comoglio, Federico and {van Arensbergen}, Joris and {van Steensel}, Bas},
  year = {2022},
