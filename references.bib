  langid = {english},
  keywords = {Computational biology and bioinformatics,Gene regulation,High-throughput screening,Mutagenesis},
  file = {/Users/u0138640/Zotero/storage/C3TKJ957/de Almeida et al. - 2022 - DeepSTARR predicts enhancer activity from DNA sequence and enables the de novo design of synthetic e.pdf}
}

@article{dealmeidaTargetedDesignSynthetic2024,
  title = {Targeted Design of Synthetic Enhancers for Selected Tissues in the {{Drosophila}} Embryo},
